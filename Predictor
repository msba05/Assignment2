public class Predictor
{
    public void studentPredictor(Student s)
    {
        if (s.getGpa() >= 3.5 && s.getCredits() >= 30){
            System.out.print("Honor Roll");
        }
        else if (s.getGpa()< 2.0){
            System.out.print("High Risk");
        }
        else System.out.print("On Track");
    }
}
