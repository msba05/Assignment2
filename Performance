public class classifyPerformance {
    public static String classifyPerformance(Student s){
        if (s.getGps() >= 3.5 && s.getCredits() >= 30){
            return "Honor Roll";
        }
        else if (s.getGpa()< 2.0){
            return "High Risk";
        }
        else return " on Track";
    }
}
